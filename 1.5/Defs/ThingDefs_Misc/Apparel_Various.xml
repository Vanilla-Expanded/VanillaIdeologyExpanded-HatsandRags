<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="ApparelMakeableBase">
    <defName>VIEHAR_Apparel_Rags</defName>
    <label>rags</label>
    <description>A set of rags meant to cover oneself, offers almost no protection or warmth, but meant to convey detachment from life's pleasures and peace with oneself.</description>
    <graphicData>
      <texPath>Things/Apparel/Rags/Rags</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costStuffCount>50</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToMake>2800</WorkToMake>
      <Mass>0.75</Mass>
      <StuffEffectMultiplierArmor>0.1</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.05</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.05</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>1.5</EquipDelay>
    </statBases>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Shoulders</li>
        <li>Arms</li>
        <li>Legs</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Apparel/Rags/Rags</wornGraphicPath>
      <layers>
        <li>Shell</li>
      </layers>
      <defaultOutfitTags>
        <li>Worker</li>
      </defaultOutfitTags>
	  <canBeDesiredForIdeo>false</canBeDesiredForIdeo>
      <canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
    </apparel>
    <colorGenerator Class="ColorGenerator_StandardApparel" />
  </ThingDef>
  
</Defs>